<mjml>
  <mj-head>
    <mj-style>
      * {
      box-sizing: border-box;
      }

      body {
      font-family: 'Inter', Helvetica Neue, Helvetica, Arial, sans-serif;
      }

      a,
      a:visited {
      color: #8d8d8d;
      text-decoration: none;
      }

      .header-img {
      text-align: left!important;
      }

      .header-txt-col div {
      text-align: right!important;
      }

      .hero-bg-image {
      background-image: url("https://emaillove-emails.s3.amazonaws.com/540acdcff690c8037a8aae5e8e6704c1.png");
      background-repeat: no-repeat;
      background-position: top center;
      background-color: #1b1b1b;
      width: 100%;
      }

      .info-mj-col {
      padding: 60px 0 30px!important;
      }

      .footer-links-col {
      min-width: 90% !important;
      display: inline-block !important;
      padding: 0 !important;
      }

      .footer-links {
      height: 100% !important;
      }

      .footer-img-col {
      display: inline-block !important;
      width: 10% !important;
      }

      .footer-img {
      padding: 0 !important;
      text-align: left !important;
      }

      @media screen and (max-width: 550px) {
      .header-section {
      text-align: center!important;
      min-width: 100%!important;
      }

      .header-img-col {
      width: 100%!important;
      display: block!important;
      text-align: center!important;
      margin: auto!important;
      padding: 0 0 6px 0!important;
      }

      .header-img {
      text-align: center!important;
      display: inline-block!important;
      padding: 0!important;
      }

      .header-img img {
      width: 100%!important;
      text-align: center!important;
      margin: auto!important;
      }

      .header-txt-col {
      width: 100%!important;
      display: block!important;
      text-align: center!important;
      margin: auto!important;
      padding: 0!important;
      }

      .header-txt div {
      text-align: center!important;
      padding: 0!important;
      }

      .hero-btns-tr > td {
      display: block!important;
      width: 100%!important;
      }

      .hero-btns-tr a {
      display: block!important;
      width: 100%!important;
      margin-bottom: 15px!important;
      text-align: center!important;
      }

      .info-mj-col {
      padding: 0!important;
      }

      .info-btns-tr > td {
      display: block!important;
      width: 100%!important;
      }

      .info-btns-tr a {
      display: block!important;
      width: 100%!important;
      margin-bottom: 15px!important;
      text-align: center!important;
      }

      .info-tr > td {
      display: block!important;
      text-align: center!important;
      margin-bottom: 4px!important;
      }

      hr {
      width: 100% !important;
      }

      .footer-links-col {
      min-width: 100% !important;
      display: block !important;
      }

      .footer-links a {
      display: block !important;
      text-align: center !important;
      margin: 0 0 14px !important;
      }

      .footer-links p {
      display: none !important;
      }

      .footer-img-col {
      width: 100%!important;
      display: block!important;
      text-align: center!important;
      margin: auto!important;
      padding: 6px 0 0 0!important;
      }

      .footer-img {
      text-align: center!important;
      display: inline-block!important;
      padding: 0!important;
      }

      .footer-img img {
      width: 100%!important;
      text-align: center!important;
      margin: auto!important;
      }
      }
    </mj-style>
    <mj-style inline="inline">
    </mj-style>
  </mj-head>
